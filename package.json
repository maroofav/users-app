{
  "name": "users-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "eslint-check": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 1",
    "eslint-fix": "eslint src --fix --ext .js,.jsx,.ts,.tsx",
    "style-lint-check": "stylelint src/components/**/*.{css,scss}",
    "style-lint-fix": "stylelint src/components/**/*.{css,scss} --fix",
    "prettier-check": "prettier --check src --ignore-unknown",
    "prettier-fix": "prettier --write src --ignore-unknown",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.6",
    "@mui/icons-material": "^6.4.12",
    "@mui/material": "^6.4.12",
    "@mui/x-data-grid": "^7.27.0",
    "@mui/x-data-grid-generator": "^7.27.0",
    "@reduxjs/toolkit": "^2.5.1",
    "@tailwindcss/postcss": "^4.0.8",
    "@tailwindcss/vite": "^4.0.8",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.2.0",
    "react-toastify": "^11.0.5",
    "redux-persist": "^6.0.0",
    "sass": "^1.71.1",
    "tailwindcss": "^4.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "3.6.2",
    "stylelint": "^16.21.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "pnpm eslint-check",
      "pnpm prettier-check"
    ],
    "*.{css,scss,md,html,json}": [
      "pnpm prettier-check"
    ],
    "*.{scss}": []
  }
}
